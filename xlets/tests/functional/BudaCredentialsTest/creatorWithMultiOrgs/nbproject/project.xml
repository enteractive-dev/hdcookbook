<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>CreatorWithMultiOrgs</name>
            <properties/>
            <folders>
                <source-folder>
                    <label>src</label>
                    <type>java</type>
                    <location>src</location>
                </source-folder>
                <source-folder>
                    <label>xlet_src</label>
                    <type>java</type>
                    <location>xlet_src</location>
                </source-folder>
                <source-folder>
                    <label>se_src</label>
                    <type>java</type>
                    <location>se_src</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>compile</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="run">
                    <target>run-grinview</target>
                </action>
                <action name="test">
                    <target>test</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>compile</target>
                </action>
            </ide-actions>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src</label>
                        <location>src</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>xlet_src</label>
                        <location>xlet_src</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>se_src</label>
                        <location>se_src</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="run"/>
                    <ide-action name="test"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>src</package-root>
                <source-level>1.3</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>xlet_src</package-root>
                <source-level>1.3</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>se_src</package-root>
                <source-level>1.3</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
